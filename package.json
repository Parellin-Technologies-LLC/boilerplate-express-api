{
    "name": "boilerplate-express-api",
    "version": "0.0.0",
    "description": "Boilerplate ExpressJS API",
    "main": "index.js",
    "scripts": {
        "test": "mocha",
        "start": "node process.js",
        "reload": "pm2 reload boilerplate-express-api",
        "restart": "pm2 restart boilerplate-express-api",
        "stop": "pm2 stop boilerplate-express-api",
        "delete": "pm2 delete boilerplate-express-api",
        "prebuild": "~/nodec -v",
        "build": "~/nodec --tmpdir=../.tmp -o api.app ./process.js"
    },
    "keywords": [
        "express",
        "api",
        "boilerplate"
    ],
    "author": "Nick Soggin",
    "repository": "https://github.com/Parellin-Technologies-LLC/boilerplate-express-api.git",
    "license": "MIT",
    "dependencies": {
        "ansi-styles": "^3.2.0",
        "body-parser": "^1.18.2",
        "compression": "^1.7.1",
        "express": "^4.16.2",
        "http-response-class": "^1.0.7",
        "method-override": "^2.3.10",
        "pm2": "^2.7.2",
        "pmx": "^1.5.4",
        "redirect-https": "^1.1.4",
        "uuid": "^3.1.0"
    },
    "devDependencies": {}
}
